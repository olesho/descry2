<Pattern title="WebdealautoSpider" storage="mysql">
	<URLRules>
		<RegexInclude>^(http://webdealauto.com)</RegexInclude>
	</URLRules>
	<Field title="Link">
		<XPath>//section/div[contains(@class,'sitemap-content') and contains(@class,'last')]//a/@href</XPath>
		<Type name="[]string" />
		<DataRules>
			<RegexInclude>^(/fr/)</RegexInclude>
		</DataRules>
	</Field>
</Pattern>				
