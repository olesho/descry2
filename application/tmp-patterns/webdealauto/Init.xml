<Pattern title="WebdealautoInit" storage="mysql">
	<URLRules>
		<RegexInclude>^(http://webdealauto.com)</RegexInclude>
	</URLRules>
	<Field title="Link">
		<XPath>//div[contains(@class,'category') and contains(@class, 'col-sm-4')]/div/ul/li/a/@href</XPath>
		<Type name="[]string" />
		<DataRules>
			<RegexInclude>^(/fr/)</RegexInclude>
		</DataRules>
	</Field>
</Pattern>				
